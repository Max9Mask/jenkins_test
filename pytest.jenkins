pipeline {
   agent any

   stages {
	stage('install packages') {
		steps {
	  		sh 'apt-get install python3-pytest'
			sh 'apt-get install python3-pymssql'
		}
	}
    stage('run tests') {
        steps{      
            sh 'pytest --alluredir=allure-report pytest_cases.py'
	    sh 'allure serve allure-report'
        }
        }

  }
}
